<?xml version="1.0" encoding="UTF-8"?>
<launch>
	<filename>sick_mrs_6xxx.launch</filename>
	<paramList>
		<!-- Set the ip address to the ip address of our MRS6124 scanner -->
	<param name="hostname" type="string" value="192.168.0.24" />
	<param name="filter_echos" type="int" value="0" />
	</paramList>
	<resultList>
		<param name="shotsPerLayer" type="int" value="924" />
		<param name="pointCloud2Height" type="int" value="96" /><!-- 24*4 24 Layers*Echos-->
		<param name="RSSIEnabled" type="bool" value="True" />
		<param name="scanRate" type="double" value="10.0" valueMin="9.0" valueMax="11.0" />
	</resultList>
</launch>
